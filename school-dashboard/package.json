{
  "name": "school-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/jsx-props-no-spreading": 0
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@apollo/link-error": "^2.0.0-beta.3",
    "apollo-upload-client": "^14.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.12.0",
    "downshift": "^6.1.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "lodash.debounce": "^4.0.8",
    "next": "10.0.7",
    "next-with-apollo": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "^3.12.0",
    "react-table": "^7.6.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "install": "0.13.0",
    "npm": "7.6.0",
    "prettier": "2.2.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
